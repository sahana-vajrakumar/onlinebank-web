<%= form_for @account do |f| %>


  <fieldset>
    <%= f.label :balance %>
    <%= f.text_field :balance, :class => "form-control" %>
  </fieldset>

  <fieldset>
    <%= f.label :account_type %>
    <%= f.text_field :account_type, :class => "form-control" %>
  </fieldset>

  <%= f.submit %>

<% end %>
